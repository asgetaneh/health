</div>
<div class="form-group">
	{{ form_label(form.managedBy," ManagedBy") }}
	{{ form_widget(form.managedBy,{'attr':{ 'class': 'form-control select2'}}) }}

</div>

<div class="form-group">
	{{ form_label(form.officeGroup,"Category") }}
	{{ form_widget(form.officeGroup,{'attr':{ 'class': 'form-control select2'}}) }}

</div>

