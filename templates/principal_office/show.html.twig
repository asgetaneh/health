{% extends 'base.html.twig' %}

{% block title %}PrincipalOffice{% endblock %}

{% block body %}
    <h1>PrincipalOffice</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ principal_office.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ principal_office.name }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ principal_office.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreateAt</th>
                <td>{{ principal_office.createAt ? principal_office.createAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ principal_office.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('principal_office_index') }}">back to list</a>

    <a href="{{ path('principal_office_edit', {'id': principal_office.id}) }}">edit</a>

    {{ include('principal_office/_delete_form.html.twig') }}
{% endblock %}
