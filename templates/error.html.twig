<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>
			MIS
			{% block title %}
				Dashboard
			{% endblock %}
		</title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link
		rel="stylesheet" href="{{ asset('assets/plugins/fontawesome-free/css/all.min.css')}}">
		{# <link rel="stylesheet" href="{{ asset('https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css')}}"> #}
		<link rel="stylesheet" href="{{ asset('assets/dist/css/adminlte.min.css')}}">
	</head>
	<body class="hold-transition  layout-top-nav hold-transition  ">
		<div class="wrapper">
			<div class="content-wrapper">
				<div class="content-header">
					<div class="container-fluid">

						<div class="row ">
							{% block noBreadcrumb %}
								<div class="col-sm-6">
									<h1 class="m-0 text-dark">
										{% block maintitle %}

											Dashboard
											{% trans %}
											Index
											{% endtrans %}
										{% endblock %}

									</h1>
								</div>
								<div class="col-sm-6">
									<ol class="breadcrumb float-sm-right">
										<li class="breadcrumb-item">
											<a href="#">Home</a>
										</li>
										{% block breadcrumb %}

											<li class="breadcrumb-item active">Dashboard v1</li>

										{% endblock %}
									</ol>
								</div>
							{% endblock %}
						</div>

					</div>
				</div>

				<section class="content">
					<div class="container-fluid">
						<div class="row">

							<div class="col-md-12">

								{# {% for label, messages in app.flashes %}
									{% for message in messages %}
										<div class="row alert alert-{{ label }} alert-dismissible">
											<button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
											<h4 class="mr-4">
												<i class="icon fa fa-{% if label=='success' %}check {% else %}ban {% endif %}"></i>
												Alert!</h4>
											{{ message }}

										</div>


									{% endfor %}
								{% endfor %} #}

							</div>
						</div>
						<center>
							<div class="row">
								<div class="col-md-12">
									<img src="{{ asset('logo.png')}}" class="d-block"/> {% block body %}{% endblock %}
									</div>
								</div>
							</center>
						</section>

					</div>


				</div>


				<script src="{{asset('assets/plugins/jquery/jquery.min.js')}}"></script>


				<script src="{{asset('assets/dist/js/adminlte.js')}}"></script>

				<script src="{{asset('assets/plugins/bootstrap/js/bootstrap.bundle.min.js')}}"></script>


				{% block javascripts %}{% endblock %}
			</body>
		</body>
	</html>
</body></html>
