     <div id="myModal" class="modal fade" role="dialog">
		<div class="modal-dialog">


			<div class="modal-content">
				<div class="modal-header bg-primary">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">{% trans %}Select Year and Office{% endtrans %}</h4>
				</div>
				<div class="modal-body">
					<form id="sample-form" method="get" action="{{path('plan_index')}}">
						<div class="form-group">
							<label class="control-label col-xs-12 col-sm-12 ">
								{% trans %}Select Planning Year{% endtrans %}
							</label>

							<select class="select2" name="planyear" id="planphase">
								{% for planningYear in planningYears %}
									<option value="{{ planningYear.id }}">
										{{ planningYear.year|date('Y') }}
									</option>
								{% endfor %}
							</select>

						</div>
						<div class="form-group">
							<label class="control-label col-xs-12 col-sm-12 ">
								Select office
							</label>

							<select class="select2" name="office" id="">
								{% for office in offices %}
									<option value="{{office.id}}">{{office}}</option>

								{% endfor %}
							</select>

						</div>
					</div>

					<div class="modal-footer">
						<button type="button" class="btn btn-default pull-left " data-dismiss="modal">Cancel</button>

						<button type="submit" class="btn btn-primary">{% trans %}submit{% endtrans %}</button>

					</form>

				</div>
			</div>
		</div>
	</div>
   
