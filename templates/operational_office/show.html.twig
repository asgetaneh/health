{% extends 'base.html.twig' %}

{% block title %}OperationalOffice{% endblock %}

{% block body %}
    <h1>OperationalOffice</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ operational_office.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ operational_office.name }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ operational_office.createdAt ? operational_office.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ operational_office.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ operational_office.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('operational_office_index') }}">back to list</a>

    <a href="{{ path('operational_office_edit', {'id': operational_office.id}) }}">edit</a>

    {{ include('operational_office/_delete_form.html.twig') }}
{% endblock %}
