{% extends 'base.html.twig' %}

{% block title %}UserGroup
{% endblock %}

{% block body %}

	<div
		class="row">
		<!-- [ sample-page ] start -->
		<div class="col-sm-12">
			<div class="card">

				<div class="card-header">
					<h5>
						Show UserGroup</h5>
					<div class="card-header-right">
						<a href="{{ path('user_group_index') }}" class="btn btn-success">back to list</a>


					</div>
				</div>
				<div class="card-body">
					<table class="table">
						<tbody>
							<tr>
								<th>Id</th>
								<td>{{ user_group.id }}</td>
							</tr>
							<tr>
								<th>Name</th>
								<td>{{ user_group.name }}</td>
							</tr>
							<tr>
								<th>Description</th>
								<td>{{ user_group.description }}</td>
							</tr>
							<tr>
								<th>CreatedAt</th>
								<td>{{ user_group.createdAt ? user_group.createdAt|date('Y-m-d H:i:s') : '' }}</td>
							</tr>
							<tr>
								<th>UpdatedAt</th>
								<td>{{ user_group.updatedAt ? user_group.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
							</tr>
							<tr>
								<th>IsActive</th>
								<td>{{ user_group.isActive ? 'Yes' : 'No' }}</td>
							</tr>
						</tbody>
					</table>

					<a href="{{ path('user_group_edit', {'id': user_group.id}) }}" class="btn btn-warning">edit</a>
					{{ include('user_group/_delete_form.html.twig') }}

				</div>
			</div>
		</div>
		<!-- [ sample-page ] end -->
	</div>

{% endblock %}
