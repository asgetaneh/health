

{% extends 'base.html.twig' %}

{% block title %}Edit Objective
{% endblock %}

{% block body %}
	<div class="row">
		<div class="col-md-12 col-sm-12 widget-container-col" id="widget-container-col-8">
			<div class="widget-box widget-color-blue" id="widget-box-8">
				<div class="widget-header">
					<h5 class="widget-title bigger lighter">Edit Objective
					</h5>

					<div class="widget-toolbar">
						<span class="badge badge-info">
							<a href="{{ path('objective_index') }}" class="btn btn-success">
								{% trans %}
								back
								{% endtrans %}
							</a>
						</span>
					</div>

				</div>

				<div class="widget-body">
					<div class="widget-toolbox" id="widget-toolbox-1">
						<div class="btn-toolbar"></div>
					</div>

					<div class="widget-main padding-16">

						<div class="table-responsive">


							{{ include('objective/_form.html.twig', {'button_label': 'Update'}) }}
							<div class="col-md-8">
								{{ include('objective/_delete_form.html.twig') }}
							</div>
						</div>

					</div>

				</div>
			</div>
		</div>
	</div>


{% endblock %}

