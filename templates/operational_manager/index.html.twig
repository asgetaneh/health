{% extends 'base.html.twig' %}

{% block title %}OperationalManager index{% endblock %}

{% block body %}
    <h1>OperationalManager index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>IsActive</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for operational_manager in operational_managers %}
            <tr>
                <td>{{ operational_manager.id }}</td>
                <td>{{ operational_manager.isActive ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('operational_manager_show', {'id': operational_manager.id}) }}">show</a>
                    <a href="{{ path('operational_manager_edit', {'id': operational_manager.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('operational_manager_new') }}">Create new</a>
{% endblock %}
